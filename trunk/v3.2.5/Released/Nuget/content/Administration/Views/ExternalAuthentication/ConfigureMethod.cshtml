@model Aaron.Admin.Models.ExternalAuthentication.AuthenticationMethodModel
@{
    //page title
    ViewBag.Title = "Cấu hình - " + @Model.FriendlyName;
}
<div class="section-header">
    <div class="title">
        <img src="@Url.Content("~/Administration/Content/images/ico-configuration.png")" alt="" />
        Cấu hình - @Model.FriendlyName
        @Html.ActionLink("(Trở về danh sách)", "Methods")
    </div>
</div>
@if (!String.IsNullOrEmpty(Model.ConfigurationActionName))
{
    @Html.Action(Model.ConfigurationActionName, Model.ConfigurationControllerName, Model.ConfigurationRouteValues);
}
else
{
    <span>Cấu hình không yêu cầu</span>
}